import java.util.*;
import java.io.*;
public class Main 
{
        public static void main (String[] args)
        {
            Scanner sc =new Scanner(System.in);
            System.out.println("enter the user name");
            String u1=sc.nextLine();
             String u2=sc.nextLine();
             if(u1 == u2)
             {
                    System.out.println("valid");
             }
             else{
                 System.out.println("invalid");
             }
        }
}
