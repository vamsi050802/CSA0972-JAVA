import java.util.*;
import java.io.*;
class Main 
{
public static void main (String[] args)
    {
        Scanner sc =new Scanner(System.in);
        System.out.println("enter the number");
        int n=sc.nextInt();
        int i,j;
        for(i=1;i<=n;i++)
        {
            System.out.println(" ");
            for(j=1;j<=i;j++)
            {
                System.out.print("*");
            }
        }
    }
}
